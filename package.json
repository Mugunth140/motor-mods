{
  "name": "desktop",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "tauri:dev": "npm run tauri -- dev",
    "build": "tsc && vite build",
    "build:frontend": "npm run build",
    "build:desktop": "npm run tauri -- build",
    "preview": "vite preview",
    "tauri": "tauri",
    "reset-db": "sh -c \"find . -maxdepth 4 -type f -name 'motormods*.db' -print -delete || true\"",
    "seed-db": "sh ./scripts/seed-db.sh",
    "help:dev": "sh -c \"echo \"Dev commands:\n  npm run dev:web        # start Vite dev server (web-only)\n  npm run dev:desktop    # start Tauri (frontend + Rust)\nBuild commands:\n  npm run build:frontend # build frontend\n  npm run build:desktop  # build installers via Tauri\nUtilities:\n  npm run reset-db       # remove local motormods db files (next run will reseed)\n  npm run seed-db        # seed the sqlite DB (desktop)\"\""
  },
  "dependencies": {
    "@tauri-apps/api": "^2",
    "@tauri-apps/plugin-dialog": "^2.5.0",
    "@tauri-apps/plugin-fs": "^2.4.5",
    "@tauri-apps/plugin-opener": "^2",
    "@tauri-apps/plugin-process": "^2.3.1",
    "@tauri-apps/plugin-sql": "^2.3.1",
    "firebase": "^12.8.0",
    "jspdf": "^4.0.0",
    "jspdf-autotable": "^5.0.2",
    "lucide-react": "^0.562.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-virtuoso": "^4.18.1",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.18",
    "@tauri-apps/cli": "^2",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^4.6.0",
    "autoprefixer": "^10.4.23",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "typescript": "~5.8.3",
    "vite": "^7.0.4"
  }
}